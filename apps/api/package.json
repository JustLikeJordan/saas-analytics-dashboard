{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:run": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "express": "^5.1.0",
    "drizzle-orm": "^0.45.0",
    "postgres": "^3.4.0",
    "ioredis": "^5.6.0",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "zod": "^3.24.0",
    "jose": "^6.0.0",
    "shared": "workspace:*"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.0.0",
    "drizzle-kit": "^0.31.0",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.0",
    "tsc-alias": "^1.8.0",
    "vitest": "^3.0.0"
  }
}
